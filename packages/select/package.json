{
  "name": "@smui/select",
  "version": "2.0.0-beta.1",
  "description": "Svelte Material UI - Select",
  "keywords": [
    "svelte",
    "svelte3",
    "material-ui",
    "material-design",
    "material",
    "svelte-components",
    "sveltejs"
  ],
  "scripts": {
    "#prepare": "echo \"$(npx glob '**/*' | sed 's/^/-I /' | paste -s -d ' ' -) --no-source-map _style.scss bare.css\" | xargs sass && echo \"$(npx glob '**/*' | sed 's/^/-I /' | paste -s -d ' ' -) --no-source-map helper-text/_style.scss helper-text/bare.css\" | xargs sass && echo \"$(npx glob '**/*' | sed 's/^/-I /' | paste -s -d ' ' -) --no-source-map icon/_style.scss icon/bare.css\" | xargs sass",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hperrin/svelte-material-ui.git"
  },
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "bugs": {
    "url": "https://github.com/hperrin/svelte-material-ui/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@material/feature-targeting": "^10.0.0",
    "@material/ripple": "^10.0.0",
    "@material/rtl": "^10.0.0",
    "@material/select": "^10.0.0",
    "@material/theme": "^10.0.0",
    "@smui/common": "file:../common",
    "@smui/floating-label": "file:../floating-label",
    "@smui/line-ripple": "file:../line-ripple",
    "@smui/list": "file:../list",
    "@smui/menu": "file:../menu",
    "@smui/notched-outline": "file:../notched-outline"
  },
  "devDependencies": {
    "glob-cmd": "0.0.7",
    "sass": "^1.32.8"
  }
}
