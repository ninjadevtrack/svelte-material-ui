{
  "name": "svelte-material-ui",
  "version": "5.0.0",
  "description": "Svelte Material UI Components",
  "keywords": [
    "svelte",
    "svelte3",
    "material-ui",
    "material-design",
    "material",
    "svelte-components",
    "sveltejs"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist --nohoist \"@tsconfig/svelte\"",
    "check": "lerna exec svelte-check",
    "clean": "lerna run clean && npm run clean:css",
    "clean:css": "test -f bare.css && rm -r bare.css || true",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "pages": "npm run bootstrap && npm run pages:build && npm run pages:push",
    "pages:build": "cd packages/site && npm run build && cd ../..",
    "pages:push": "git add -f packages/site/build && git commit -m 'chore: update built files' && git push origin $(git subtree split --prefix packages/site/build master):gh-pages --force && git reset --hard HEAD^",
    "prepare": "husky install && npm run build:css",
    "build:css": "smui-theme compile bare.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "bare.css"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hperrin/svelte-material-ui.git"
  },
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "bugs": {
    "url": "https://github.com/hperrin/svelte-material-ui/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@smui/banner": "file:packages/banner",
    "@smui/button": "file:packages/button",
    "@smui/card": "file:packages/card",
    "@smui/checkbox": "file:packages/checkbox",
    "@smui/chips": "file:packages/chips",
    "@smui/circular-progress": "file:packages/circular-progress",
    "@smui/common": "file:packages/common",
    "@smui/data-table": "file:packages/data-table",
    "@smui/dialog": "file:packages/dialog",
    "@smui/drawer": "file:packages/drawer",
    "@smui/fab": "file:packages/fab",
    "@smui/floating-label": "file:packages/floating-label",
    "@smui/form-field": "file:packages/form-field",
    "@smui/icon-button": "file:packages/icon-button",
    "@smui/image-list": "file:packages/image-list",
    "@smui/layout-grid": "file:packages/layout-grid",
    "@smui/line-ripple": "file:packages/line-ripple",
    "@smui/linear-progress": "file:packages/linear-progress",
    "@smui/list": "file:packages/list",
    "@smui/menu": "file:packages/menu",
    "@smui/menu-surface": "file:packages/menu-surface",
    "@smui/notched-outline": "file:packages/notched-outline",
    "@smui/paper": "file:packages/paper",
    "@smui/radio": "file:packages/radio",
    "@smui/ripple": "file:packages/ripple",
    "@smui/segmented-button": "file:packages/segmented-button",
    "@smui/select": "file:packages/select",
    "@smui/slider": "file:packages/slider",
    "@smui/snackbar": "file:packages/snackbar",
    "@smui/switch": "file:packages/switch",
    "@smui/tab": "file:packages/tab",
    "@smui/tab-bar": "file:packages/tab-bar",
    "@smui/tab-indicator": "file:packages/tab-indicator",
    "@smui/tab-scroller": "file:packages/tab-scroller",
    "@smui/textfield": "file:packages/textfield",
    "@smui/tooltip": "file:packages/tooltip",
    "@smui/top-app-bar": "file:packages/top-app-bar",
    "@smui/touch-target": "file:packagestouch-target/"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "husky": "^7.0.4",
    "lerna": "^4.0.0",
    "prettier": "~2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "sass": "^1.43.4",
    "smui-theme": "file:packages/smui-theme",
    "svelte": "^3.35.0",
    "svelte-check": "^2.2.8"
  }
}
